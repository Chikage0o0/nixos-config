    # hk_node = "vmess://...";
  };

  # 订阅链接列表
  daeSubscriptions = [
    # "https://example.com/subscribe"
  ];

  # Hosts 配置
  extraHosts = {
    # "1.1.1.1" = [ "example.com" ];
  };
}
